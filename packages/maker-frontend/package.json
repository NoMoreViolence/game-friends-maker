  "scripts": {
    "dev": "cross-env REACT_APP_STAGE=dev REACT_APP_RUNNING_ENV=web PORT=3000 react-scripts start",
    "start:test": "cross-env REACT_APP_STAGE=test REACT_APP_RUNNING_ENV=web PORT=3000 react-scripts start",
    "build:test": "cross-env REACT_APP_STAGE=test REACT_APP_RUNNING_ENV=web react-scripts build",
    "build:prod": "cross-env REACT_APP_STAGE=prod REACT_APP_RUNNING_ENV=web react-scripts build",
    "deploy:prod": "aws s3 sync ./build s3://cohope.io --profile=cohope",
    "clear:prod": "aws cloudfront create-invalidation --profile=cohope --distribution-id E273TT4DOWJUI4 --paths / /index.html /service-worker.js /manifest.json /favicon.svg /favicon.png",
    "deploy:test": "aws s3 sync ./build s3://staging.cohope.io --profile=cohope",
    "clear:test": "aws cloudfront create-invalidation --profile=cohope --distribution-id E2VOHNAS0N98KS --paths / /index.html /service-worker.js /manifest.json /favicon.svg /favicon.png",
    "codegen:schema": "rimraf 'src/schema.json' && apollo client:download-schema --endpoint=http://localhost:7000/local/graphql src/schema.json",
    "codegen:types": "rimraf './src/**/__generated__' && apollo client:codegen --addTypename --includes=src/graphqls/**/*.ts --globalTypesFile=src/graphqls/__generated__/globalTypes.ts --endpoint=http://localhost:7000/local/graphql --target=typescript"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^1.15.0",
