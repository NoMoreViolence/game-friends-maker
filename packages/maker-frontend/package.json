  "scripts": {
    "dev": "cross-env REACT_APP_STAGE=dev REACT_APP_RUNNING_ENV=web PORT=3000 react-scripts start",
    "start:test": "cross-env REACT_APP_STAGE=test REACT_APP_RUNNING_ENV=web PORT=3000 react-scripts start",
    "build:test": "cross-env REACT_APP_STAGE=test REACT_APP_RUNNING_ENV=web react-scripts build",
    "build:prod": "cross-env REACT_APP_STAGE=prod REACT_APP_RUNNING_ENV=web react-scripts build",
    "deploy:prod": "aws s3 sync ./build s3://cohope.io --profile=coop-dev-s3",
    "clear:prod": "aws cloudfront create-invalidation --profile=coop-dev-s3 --distribution-id E273TT4DOWJUI4 --paths / /index.html /service-worker.js /manifest.json /favicon.svg /favicon.png",
    "deploy:dev": "aws s3 sync ./build s3://coopnow.net --profile=coop-dev-s3",
    "clear:dev": "aws cloudfront create-invalidation --profile=coop-dev-s3 --distribution-id E179967HJCF1UW --paths / /index.html /service-worker.js /manifest.json /favicon.svg /favicon.png",
    "codegen:schema": "rimraf 'src/schema.json' && apollo client:download-schema --endpoint=http://localhost:7000/graphql src/schema.json",
    "codegen:types": "rimraf './src/**/__generated__' && apollo client:codegen --addTypename --includes=src/graphqls/**/*.ts --globalTypesFile=src/graphqls/__generated__/globalTypes.ts --endpoint=http://localhost:7000/graphql --target=typescript"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^1.8.3",
    "@graphql-codegen/fragment-matcher": "1.8.3",
    "@graphql-codegen/introspection": "1.8.3",
    "@graphql-codegen/typescript": "1.8.3",
    "@graphql-codegen/typescript-document-nodes": "1.8.3",
