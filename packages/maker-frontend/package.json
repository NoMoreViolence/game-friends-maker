    "@material-ui/core": "^4.10.1",
    "@material-ui/icons": "^4.9.1",
    "@material-ui/lab": "^4.0.0-alpha.49",
    "@types/jest": "^25.2.3",
    "@types/node": "^14.0.9",
    "@types/react": "^16.9.0",
    "@types/react-dom": "^16.9.0",
    "apollo": "^2.28.0",
    "apollo-boost": "^0.4.9",
    "apollo-cache-inmemory": "^1.6.6",
    "apollo-link": "^1.2.14",
    "apollo-link-context": "^1.0.20",
    "apollo-link-error": "^1.1.13",
    "apollo-link-http": "^1.5.17",
    "axios": "^0.19.2",
    "bson": "^4.0.4",
    "dayjs": "^1.8.28",
    "graphql": "^14.5.8",
    "lodash": "^4.17.15",
    "node-sass": "^4.14.1",
    "polished": "^3.6.5",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-google-login": "^5.1.21",
    "react-helmet": "^6.0.0",
    "react-intl": "^4.6.3",
    "react-router-dom": "^5.2.0",
    "react-scripts": "3.4.1",
    "react-use": "^15.3.0",
    "rxjs": "^6.5.5",
    "styled-components": "^5.1.1",
    "styled-system": "^5.1.5",
    "sweetalert2": "^9.17.1",
    "sweetalert2-react-content": "^3.0.3",
    "typescript": "~3.7.2",
    "use-react-router": "^1.0.7",
    "utility-types": "^3.10.0"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^1.15.0",
    "@graphql-codegen/fragment-matcher": "^1.15.0",
    "@graphql-codegen/introspection": "^1.15.0",
    "@graphql-codegen/typescript": "^1.15.0",
    "@graphql-codegen/typescript-document-nodes": "^1.15.0",
    "@graphql-codegen/typescript-graphql-files-modules": "^1.15.0",
    "@graphql-codegen/typescript-operations": "^1.15.0",
    "@graphql-codegen/typescript-react-apollo": "^1.15.0",
    "@svgr/cli": "^5.4.0",
    "@types/axios": "^0.14.0",
    "@types/bson": "^4.0.2",
    "@types/lodash": "^4.14.154",
    "@types/react-helmet": "^6.0.0",
    "@types/react-router-dom": "^5.1.5",
    "@types/styled-components": "^5.1.0",
    "@types/styled-system": "^5.1.9",
    "chroma-js": "0.6.1",
    "cross-env": "^7.0.2",
    "prettier": "^2.0.5"
  },
  "scripts": {
    "dev": "cross-env REACT_APP_STAGE=dev REACT_APP_RUNNING_ENV=web PORT=3000 react-scripts start",
    "start:test": "cross-env REACT_APP_STAGE=test REACT_APP_RUNNING_ENV=web PORT=3000 react-scripts start",
    "build:test": "cross-env REACT_APP_STAGE=test REACT_APP_RUNNING_ENV=web react-scripts build",
    "build:prod": "cross-env REACT_APP_STAGE=prod REACT_APP_RUNNING_ENV=web react-scripts build",
    "deploy:prod": "aws s3 sync ./build s3://cohope.io --profile=cohope",
    "clear:prod": "aws cloudfront create-invalidation --profile=cohope --distribution-id E273TT4DOWJUI4 --paths / /index.html /service-worker.js /manifest.json /favicon.svg /favicon.png",
    "deploy:test": "aws s3 sync ./build s3://staging.cohope.io --profile=cohope",
    "clear:test": "aws cloudfront create-invalidation --profile=cohope --distribution-id E2VOHNAS0N98KS --paths / /index.html /service-worker.js /manifest.json /favicon.svg /favicon.png",
    "codegen:schema": "rimraf 'src/schema.json' && apollo client:download-schema --endpoint=http://localhost:7000/local/graphql src/schema.json",
    "codegen:types": "rimraf './src/**/__generated__' && apollo client:codegen --addTypename --includes=src/graphql/**/*.ts --globalTypesFile=src/graphql/__generated__/globalTypes.ts --endpoint=http://localhost:7000/local/graphql --target=typescript",
    "generate:svg-components": "rm -rf src/ui/Components/Icons/Generated && svgr --config-file=config/.svgrrc.json --ext=\"tsx\" --template=\"config/svgTemplate.js\" --filename-case \"pascal\" public/assets/icons --out-dir src/ui/Components/Icons/Generated --index-template=\"config/indexTemplate.js\"",
    "generate:svg-map": "node config/generate-svg-map.js",
    "generate:svg": "yarn generate:svg-components && yarn generate:svg-map && prettier --print-width 120 --ignore-path \"./../../.prettierignore\" --write \"src/**/*.ts\" \"src/**/*.tsx\""
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
