{
  "name": "cohope-backend",
  "version": "1.0.0",
  "private": true,
  "description": "Cohope Backend",
  "main": "index.js",
  "scripts": {
    "build": "rm -rf lib && tsc",
    "dev": "./node_modules/nodemon/bin/nodemon.js -e ts -w ./src -x yarn start:devStart",
    "start:devStart": "serverless offline -s local --port 7000 --websocketPort 7001 --skipCacheInvalidation",
    "deploy:dev": "yarn build && SLS_DEBUG=* serverless deploy -s dev",
    "deploy:prod": "yarn build && serverless deploy -s prod"
  },
  "dependencies": {
    "@hapi/joi": "^15.1.0",
    "apollo-error-converter": "^1.1.0",
    "apollo-server": "^2.9.8",
    "apollo-server-lambda": "^2.9.8",
    "axios": "^0.19.0",
    "file-loader": "^4.2.0",
    "graphql": "^14.5.8",
    "http-status-codes": "^1.3.2",
    "joi-extract-type": "^15.0.1",
    "jsonwebtoken": "^8.5.1",
    "mongodb": "^3.3.0",
    "mongoose": "^5.7.1",
    "mongoosejs-soft-delete": "^1.0.2",
    "nodemon": "^1.19.1",
    "randomstring": "^1.1.5",
    "reflect-metadata": "^0.1.13",
    "serverless": "1.55.1",
    "serverless-http": "^2.0.2",
    "serverless-offline": "^5.7.3",
    "serverless-webpack": "^5.2.0",
    "serverless-websockets-plugin": "^1.0.0",
    "source-map-support": "^0.5.13",
    "terser-webpack-plugin": "^1.4.1",
    "ts-loader": "^6.2.1",
    "tsconfig-paths-webpack-plugin": "^3.2.0",
    "type-graphql": "^0.17.6",
    "typedi": "^0.8.0",
    "url-loader": "^2.1.0",
    "webpack": "^4.29.0",
    "webpack-node-externals": "^1.7.2"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.35",
    "@types/dotenv": "^6.1.1",
    "@types/hapi__joi": "^15.0.3",
    "@types/http-status-codes": "^1.2.0",
    "@types/jsonwebtoken": "^8.3.3",
    "@types/mongoose": "^5.5.17",
    "@types/node": "^12.12.6",
    "@types/randomstring": "^1.1.6",
    "typescript": "^3.7.2"
  },
  "license": "MIT"
}
