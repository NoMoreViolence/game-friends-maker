{
  "name": "cohope-backend",
  "version": "1.0.0",
  "private": true,
  "description": "Cohope Backend",
  "main": "index.js",
  "scripts": {
    "build": "rm -rf lib && tsc",
    "dev": "./node_modules/nodemon/bin/nodemon.js -e ts -w ./src -x yarn start:devStart",
    "start:devStart": "serverless offline -s local --httpPort 7000 --websocketPort 7001 --skipCacheInvalidation",
    "deploy:test": "yarn build && SLS_DEBUG=* serverless deploy -s dev",
    "deploy:prod": "yarn build && serverless deploy -s prod"
  },
  "dependencies": {
    "@hapi/joi": "^17.1.1",
    "apollo-error-converter": "^1.1.1",
    "apollo-server": "^2.13.1",
    "apollo-server-lambda": "^2.13.1",
    "axios": "^0.19.2",
    "file-loader": "^6.0.0",
    "graphql": "^14.5.8",
    "http-status-codes": "^1.3.2",
    "joi-extract-type": "^15.0.1",
    "jsonwebtoken": "^8.5.1",
    "mongodb": "^3.5.7",
    "mongoose": "^5.9.16",
    "mongoosejs-soft-delete": "^1.0.2",
    "nodemon": "^2.0.4",
    "randomstring": "^1.1.5",
    "reflect-metadata": "^0.1.13",
    "serverless": "^1.71.3",
    "serverless-http": "^2.5.0",
    "serverless-offline": "^6.1.7",
    "serverless-webpack": "^5.3.2",
    "serverless-websockets-plugin": "^1.0.0",
    "source-map-support": "^0.5.19",
    "terser-webpack-plugin": "^3.0.2",
    "ts-loader": "^7.0.5",
    "tsconfig-paths-webpack-plugin": "^3.2.0",
    "type-graphql": "^0.17.6",
    "typedi": "^0.8.0",
    "url-loader": "^4.1.0",
    "webpack": "^4.43.0",
    "webpack-node-externals": "^1.7.2"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.51",
    "@types/dotenv": "^8.2.0",
    "@types/hapi__joi": "^17.1.0",
    "@types/http-status-codes": "^1.2.0",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/mongoose": "^5.7.21",
    "@types/node": "^14.0.5",
    "@types/randomstring": "^1.1.6",
    "typescript": "~3.7.2"
  },
  "license": "MIT"
}
